{
    "window.restoreWindows": "all",
    "editor.rulers": [
        80
    ],
    "editor.minimap.enabled": false,
    "window.zoomLevel": 0,
    "terminal.integrated.shell.osx": "/usr/local/bin/fish",
    "terminal.integrated.fontFamily": "Source Code Pro",
    "todo-tree.ripgrep": "/Applications/Visual Studio Code.app/Contents/Resources/app/node_modules.asar.unpacked/vscode-ripgrep/bin/rg",
    "extensions.ignoreRecommendations": false,
    "workbench.statusBar.feedback.visible": false,
    "files.associations": {
        "**/*.css": "postcss",
        "*.css": "css"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": true,
    "window.openFoldersInNewWindow": "on",
    "window.nativeTabs": true,
    "todo-tree.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "editor.matchBrackets": false,
    "workbench.colorTheme": "SynthWave '84",
    "editor.renderWhitespace": "all",
    "terminal.integrated.rendererType": "dom",
    "editor.renderControlCharacters": true,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.quickSuggestions": false
    },
    "workbench.startupEditor": "newUntitledFile",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.ripgrep.ripgrep": "/Applications/Visual Studio Code.app/Contents/Resources/app/node_modules.asar.unpacked/vscode-ripgrep/bin/rg",
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrains Mono",
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "prettier.arrowParens": "always",
    "debug.allowBreakpointsEverywhere": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.confirmSync": false,
    "git.autofetch": true,
    "python.pythonPath": "/usr/local/bin/python3",
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.cargoFeatures.loadOutDirsFromCheck": true,
    "liveshare.presence": false,
    "editor.formatOnSaveMode": "modifications",
    "tabnine.experimentalAutoImports": true,
    "python.languageServer": "Pylance",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    }
}