{
    "window.restoreWindows": "all",
    "editor.rulers": [
        80
    ],
    "editor.minimap.enabled": false,
    "window.zoomLevel": 0,
    "terminal.integrated.shell.osx": "fish",
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "**/*.css": "postcss",
        "*.css": "css",
        "*.journal": "ledger"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": true,
    "window.openFoldersInNewWindow": "on",
    "window.nativeTabs": true,
    "editor.renderWhitespace": "all",
    "terminal.integrated.rendererType": "dom",
    "editor.renderControlCharacters": true,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.quickSuggestions": false
    },
    "workbench.startupEditor": "newUntitledFile",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.ripgrep.ripgrep": "/Applications/Visual Studio Code.app/Contents/Resources/app/node_modules.asar.unpacked/vscode-ripgrep/bin/rg",
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrains Mono",
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "prettier.arrowParens": "always",
    "debug.allowBreakpointsEverywhere": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.confirmSync": false,
    "git.autofetch": true,
    "python.pythonPath": "/usr/local/bin/python3",
    "rust-analyzer.checkOnSave.command": "clippy",
    "editor.formatOnSaveMode": "modifications",
    "python.languageServer": "Pylance",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "terminal.integrated.shell.linux": "/run/current-system/sw/bin/fish",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "markdownlint.config": {
        "MD024": { "siblings_only": true }
    },
    "gitlens.defaultTimeFormat": "hh:mm",
    "gitlens.defaultDateFormat": "YY-MM-DD hh:mm",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "redhat.telemetry.enabled": false,
    "terminal.integrated.tabs.enabled": true,
    "shellcheck.executablePath": "shellcheck",
    "shellcheck.ignorePatterns": {
        "**/.envrc": true,
        "**/*.zsh": true,
        "**/*.zshrc": true,
        "**/zshrc": true,
        "**/*.zprofile": true,
        "**/zprofile": true,
        "**/*.zlogin": true,
        "**/zlogin": true,
        "**/*.zlogout": true,
        "**/zlogout": true,
        "**/*.zshenv": true,
        "**/zshenv": true,
        "**/*.zsh-theme": true
    }
}