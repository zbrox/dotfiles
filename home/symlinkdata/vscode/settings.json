{
    "window.restoreWindows": "all",
    "editor.rulers": [
        80
    ],
    "editor.minimap.enabled": false,
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "**/*.css": "postcss",
        "*.css": "css",
        "*.journal": "ledger"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": true,
    "window.openFoldersInNewWindow": "on",
    "window.nativeTabs": true,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "workbench.startupEditor": "newUntitledFile",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "#AFE1AF",
        "type": "none"
    },
    "todo-tree.ripgrep.ripgrep": "/Applications/Visual Studio Code.app/Contents/Resources/app/node_modules.asar.unpacked/vscode-ripgrep/bin/rg",
    "editor.fontLigatures": true,
    "editor.fontFamily": "Iosevka",
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    "prettier.arrowParens": "always",
    "debug.allowBreakpointsEverywhere": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.confirmSync": false,
    "git.autofetch": true,
    "editor.formatOnSaveMode": "modifications",
    "python.languageServer": "Pylance",
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
    },
    "telemetry.telemetryLevel": "off",
    "markdownlint.config": {
        "MD024": { "siblings_only": true }
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "redhat.telemetry.enabled": false,
    "terminal.integrated.tabs.enabled": true,
    "shellcheck.ignorePatterns": {
        "**/.envrc": true,
        "**/*.zsh": true,
        "**/*.zshrc": true,
        "**/zshrc": true,
        "**/*.zprofile": true,
        "**/zprofile": true,
        "**/*.zlogin": true,
        "**/zlogin": true,
        "**/*.zlogout": true,
        "**/zlogout": true,
        "**/*.zshenv": true,
        "**/zshenv": true,
        "**/*.zsh-theme": true
    },
    "shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
    "editor.suggestSelection": "first",
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": true,
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "plaintext": true,
        "markdown": false,
        "scminput": false,
        "yaml": true,
        "gleam": false
    },
    "git.openRepositoryInParentFolders": "never",
    "editor.codeLensFontFamily": "Iosevka",
    "editor.fontSize": 13,
    "svelte.enable-ts-plugin": true,
    "typst-lsp.experimentalFormatterMode": "on",
    "go.toolsManagement.autoUpdate": true,
    "editor.accessibilitySupport": "off",
    "github.copilot.editor.enableAutoCompletions": true,
    "prisma.showPrismaDataPlatformNotification": false
}